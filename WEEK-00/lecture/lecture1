> # deal with special HTML/XML characters
> 
> htmlspecialchars <- function(string) {
+ 
+     x = c("&", '"', "<", ">")
+ 
+     subx = c("&amp;", "&quot;", "&lt;", "&gt;")
+ 
+     for (i in seq_along(x)) {
+ 
+         string = gsub(x[i], subx[i], string, fixed = TRUE)
+ 
+     }
+ 
+     string
+ 
+ }
> 
> 
> 
> strFun <- 
+ 
+ '       <KeyWord name="%s" func="yes">
+ 
+            <Overload retVal="function">
+ 
+ %s
+ 
+            </Overload>
+ 
+        </KeyWord>
+ 
+ '
> 
> 
> 
> strNoFun <- 
+ 
+ '       <KeyWord name="%s" />
+ 
+ '
> 
